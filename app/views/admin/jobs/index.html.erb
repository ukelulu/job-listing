<div class="pull-right">
  <%= link_to("Add a job",new_admin_job_path,:class =>"btn btn-primary") %>
</div>
<br><br>
<table class ="table table-hover">

      <% @jobs.each do |job| %>
    <tr>
      <td>
        <%= link_to(job.title,admin_job_path(job)) %>
      </td>
      <td>
        <%= job.created_at %>
      </td>
      <td>
        <%= link_to("Edit",edit_admin_job_path(job),class: "btn btn-sm btn-default") %>
        <%= link_to("Delete",admin_job_path(job), class: "btn btn-sm btn-default" ,:method => :delete, :date =>{:confirm => "ARE YOU SURE?"}) %>
      </td>
    </tr>
    <% end %>

  </tbody>
</table>
